<div class="container mt-5">
    <h2>Sefer Bilgileri</h2>
    <form action="/admin/addUserPost" method="post" enctype="application/x-www-form-urlencoded">
      <div class="form-group">
        <label for="Saat">Kullanıcı Adı</label>
        <input type="text" name="username" class="form-control" id="seferSaati" placeholder="Kullanıcı Adı Giriniz...">
      </div>
      <div class="form-group">
        <label for="Guzergah">Şifre</label>
        <input type="password" name="password" class="form-control" id="seferGuzergahi" placeholder="Şifre giriniz...">
      </div>
      <div class="form-group">
        <label for="Guzergah">Şifre Tekrar</label>
        <input type="password" name="passwordAgain" class="form-control" id="seferGuzergahi" placeholder="Şifreyi tekrar giriniz...">
      </div>

      <button type="submit" class="btn btn-primary">Gönder</button>
    </form>
  </div>
  
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.1/dist/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
  
  </body>
  </html>